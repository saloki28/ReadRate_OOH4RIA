<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="ReadRate_e4Gen.Infraestructure"
		                   namespace="ReadRate_e4Gen.Infraestructure.EN.ReadRate_E4">
		    
		    
			
				<class name="AdministradorNH" table="Administrador"> 
				  	
						<id name="Id" column="idAdministrador" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Nombre" type="string"> 
						<column name="nombre"  not-null="true"/>
					</property>
					<property name="Pass" type="string"> 
						<column name="pass"  not-null="true"/>
					</property>
			
			
			
			
		  			
						<bag name="NoticiaAdministrada" >
							<key>
							
								
								<column name="FK_idAdministrador_idAdministrador"/>
					      	</key>
							<one-to-many class="ReadRate_e4Gen.Infraestructure.EN.ReadRate_E4.NoticiaNH"/>
					    </bag>
					
			
		  			
						<bag name="EventoAdministrado" >
							<key>
							
								
								<column name="FK_idAdministrador_idAdministrador"/>
					      	</key>
							<one-to-many class="ReadRate_e4Gen.Infraestructure.EN.ReadRate_E4.EventoNH"/>
					    </bag>
					
			
					
					
					
					
					
		
		 </class>	
		  
		  <query name="AdministradorNHReadAllHQL">from AdministradorNH</query>
		</hibernate-mapping>
	