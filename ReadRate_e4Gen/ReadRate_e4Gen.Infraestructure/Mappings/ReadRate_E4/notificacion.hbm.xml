						</many-to-one>
											 					
			
		  			
						<many-to-one name="ClubNotificado" class="ReadRate_e4Gen.Infraestructure.EN.ReadRate_E4.ClubNH" >	
								<column name="FK_idClub_idClub"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Rese単aNotificada" class="ReadRate_e4Gen.Infraestructure.EN.ReadRate_E4.Rese単aNH" >	
								<column name="FK_idRese単a_idRese単a"/> 
						</many-to-one>
											 					
			
		  			
						<bag name="AutorNotificado" table="[autor_notificacion]" inverse="true" >
							<key>
								
									<column name="FK_idNotificacion_idNotificacionAutor"/> 
							</key>
      						<many-to-many class="ReadRate_e4Gen.Infraestructure.EN.ReadRate_E4.AutorNH">
									<column name="FK_idUsuario_idAutorNotificado"/> 
