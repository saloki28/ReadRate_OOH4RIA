					</property>
					<property name="TituloResumen" type="string"> 
						<column name="tituloResumen"  not-null="true"/>
					</property>
					<property name="TextoCuerpo" type="string"> 
						<column name="textoCuerpo"  not-null="true"/>
					</property>
			
			
			
			
			
		  			
						<many-to-one name="NoticiaNotificada" class="ReadRate_e4Gen.Infraestructure.EN.ReadRate_E4.NoticiaNH" >	
								<column name="FK_idNoticia_idNoticiaNotificada"/> 
						</many-to-one>
											 					
			
		  			
